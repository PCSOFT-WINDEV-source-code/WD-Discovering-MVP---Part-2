     p_codes :
      -
        code : |1+
         Procedure PUBLIC NbItems() : int
         
         nNbArticles is int
         
         // Browses the order lines
         FOR EACH clLine of m_clArrayOrderLine.m_arrOrderLines
         	// Increments the number of products
         	nNbArticles += clLine.m_nQuantity
         END
         
         // Returns the number of products
         RESULT nNbArticles
