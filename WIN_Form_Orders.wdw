   -
     code : |1+
      Procedure WIN_Form_Orders(gpclOrder is POrderForm dynamic <presenter>)
      
      LOCAL
      // If no presenter (POrderForm) is passed to the view (WIN_Order_Form)
      IF gpclOrder = Null THEN
      	
      	// Creates the presenter (POrderForm) by specifying the model on which it must operate (morder)
      	clOrder is MOrder(Null)
      	gpclOrder = new POrderForm(clOrder)
      	
      END
   -
     code : |1+
      
      
     type : 34
   -
     code : |1+
      
      // Modifies the title of the window
      MySelf..Title = gpclOrder.NameInLetters
      
      // Refreshes the display
      SourceToScreen(MySelf, "gpclOrder")
      
      // Stores the position and the current selection
      TableSavePositionAndSelection(TABLE_ORDERLINE)
      
      // Refreshes the table
      TableDisplay(TABLE_ORDERLINE,taInit)
      
      // Sorts the table by name
      TableSort(TABLE_ORDERLINE, COL_REFERENCE..Name)
      
      // Restores the position and the selection
      TableRestorePositionAndSelection(TABLE_ORDERLINE)
