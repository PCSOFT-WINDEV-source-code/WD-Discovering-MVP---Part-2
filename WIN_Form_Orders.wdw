     p_codes :
      -
        code : |1+
         // A row must be selected
         IF TableSelect(TABLE_ORDERLINE) <= 0 THEN RETURN
         
         // Confirm the deletion
         IF Dialog("Do you want to delete this order line?", ["Delete", "Don't delete"], 1, 2, dlgIconQuestion) = 2 THEN RETURN
         
         
         clError is CManageError
         
         // Retrieves the selected order line
         pclOrdLine is MOrderLine dynamic = TABLE_ORDERLINE
         
         // Deletes the order line
         IF NOT gpclOrder.bDeleteLine(pclOrdLine, clError) THEN
         	// Displays the error in case of failure
         	Error(clError.FormatMessage())
         END
